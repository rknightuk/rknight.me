---
title: Games Collection
permalink: 'collections/games/index.html'
layout: collections
wide: true
eleventyNavigation:
  key: Games Collection
  parent: Collections
eleventyComputed:
    subtitle: "{{ catalog.games.count.games }} games across {{ catalog.games.count.platforms }} platforms"
---

<style>
    .collection-grid-container .summary-wrap {
        display: flex;
        margin-top: -35px;
        align-items: center;
    }

    .collection-grid-container .icon {
        max-height: 35px;
        max-width: 100px;
    }

    .trophies {
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      position: absolute;
      bottom: 0;
      left: 0;
      right: 0;
      background: rgba(0, 0, 0, 0.76);
      font-size: 0.8em;
      padding: 3px 10px;
    }

    .achievements {
      display: flex;
      justify-content: space-between;
    }

    .trophy {
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .trophy svg {
      width: 0.8em;
      height: 0.8em;
      margin-right: 0.3em;
    }

    .trophy-bronze {
        color: #cd7e32;
    }

    .trophy-silver {
        color: silver;
    }
    .trophy-gold {
        color: gold;
    }
    .trophy-platinum {
        color: #a1bbf8;
    }

    .trophy-xbox {
      color: green;
    }
</style>

{%- for platform in catalog.games.platforms %}
    <details class="collection-grid-container" {% if loop.index0 === 0 %}open{% endif %}>
        <summary>
            <span class="summary-wrap">
                <svg class="icon {{ platform.key }}" style="color: {{ platform.color }}"><use xlink:href="#{{ platform.key }}"></use></svg> {{ platform.name }} ({{ catalog.games.games[platform.name].length }})
            </span>
        </summary>
        <div class="almanac-grid game-type-{{ platform.name }}">
            {%- for game in catalog.games.games[platform.name] %}
              <div class="almanac-grid-item">
                <hyper-card>
                  <a href="https://thegamesdb.net/game.php?id={{ game.gameID }}">
                    <img class="almanac-poster" src="/assets/catalog/games/{{ game.gameID }}.jpg">
                  </a>
                  {{ { "title": game.title, "platform": platform.name, "trophies": catalog.trophies } | getTrophies | safe }}
                  {{ { "title": game.title, "platform": platform.name, "achievements": catalog.achievements } | getAchievements | safe }}
                </hyper-card>
              </div>
            {% endfor%}
        </div>
    </details>
{% endfor%}

