---
title: Games
permalink: 'collections/games/index.html'
layout: collections
---

<style>
    .collection-grid-container .summary-wrap {
        display: flex;
        margin-top: -35px;
        align-items: center;
    }

    .collection-grid-container .icon {
        max-height: 35px;
        max-width: 100px;
    }
</style>

<h3>{{ games.count.games }} games across {{ games.count.platforms }} platforms</h3>

{%- for platform in games.platforms %}
    <details class="collection-grid-container">
        <summary>
            <span>
                <span class="summary-wrap">
                    <svg class="icon {{ platform.key }}" style="color: {{ platform.color }}"><use xlink:href="#{{ platform.key }}"></use></svg> {{ platform.name }} ({{ games.games[platform.name].length }})
                </span>
            </span>
        </summary>
        <div class="collection-grid">
            {%- for game in games.games[platform.name] %}
                <div class="collection-item-wrapper">
                    <div class="collection-image-wrapper">
                        <a target="_blank" rel="noopener" href="https://thegamesdb.net/game.php?id={{ game.gameID }}">
                            <img src="/assets/games/{{ game.gameID }}.jpg">
                         </a>
                    </div>
                    <div class="collection-label">{{ game.title }}</div>
                </div>
            {% endfor%}
        </div>
    </details>
{% endfor%}

