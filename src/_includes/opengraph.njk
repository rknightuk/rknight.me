<html>
    <head>
        <style>
            @font-face {
                font-family: 'Cartridge-BoldRough';
                src: url('/assets/fonts/Cartridge-BoldRough.woff2 ') format('woff2');
                font-display: swap;
            }

            body {
                margin: 0;
                padding: 0; 
                background: #272523;
                display: flex;
                align-items: center;
                justify-content: center;
            }

            .root {
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
                padding: 30px;
            }

            .logo {
                display: flex;
                align-items: flex-end;
                justify-content: center;
                width: 100%;
                margin-bottom: 20px;
            }

            .logo svg {
                width: 175px;
                height: 175px;
            }

            .title {
                display: flex;
                align-items: flex-start;
                justify-content: center;
                text-align: center;
                width: 100%;
                font-size: 70px;
                font-family: 'Cartridge-BoldRough';
                color: #fff0d4;
            }

            {% if title === 'robb knight' %}
                .title {
                    font-feature-settings: 'ss01';
                }
            {% endif %}
        </style>
    </head>
